<%= turbo_frame_tag "template_images_upload" do %>
  <% (template_images["default_area"] || []).each do |image_data| %>
    <%= image_tag image_data[:url], 
      data: { 
        image_id: image_data[:id],
        action: "dragstart->drop-template-image#dragStart"
      }, 
      crossorigin: "anonymous"
    %>
  <% end %>
<% end %>
